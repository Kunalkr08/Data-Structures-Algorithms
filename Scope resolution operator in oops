#include<bits/stdc++.h>
using namespace std;
#define ll long long
#define mod 1000000007

class A{
public:
      ll x;
      A();
      friend class B;
};

A::A(){
   x=5;
}

class B{
      public:
         void display(A &a){
             cout<<"Value of x is:"<<" "<<a.x<<endl;
         }
};
int main(){ 
     
     A a;
     B b;
     b.display(a);
     return 0;
}
