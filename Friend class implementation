#include<bits/stdc++.h>
using namespace std;
#define ll long long
#define mod 1000000007

class A{
      ll x=5;
      friend class B;
};

class B{
      public:
         void display(A &a){
             cout<<"Value of x is:"<<" "<<a.x<<endl;
         }
};
int main(){ 
     
     A a;
     B b;
     b.display(a);
     return 0;
}
